@model fullstackCsharp.Models.Attendane

@{
    ViewData["Title"] = "Details";
}

<h1>Chi tiết</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên nhân viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdUNavigation.IdU)
        </dd>
        <dt class="col-sm-2">
            Ngày
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AttendaneDate)
        </dd>
        <dt class="col-sm-2">
            Giờ vào
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Checkin)
        </dd>
        <dt class="col-sm-2">
            Giở ra
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Checkout)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.IdA">Sửa</a> |
    <a class="btn btn-outline-info" asp-action="Index">Trở lại</a>
</div>
