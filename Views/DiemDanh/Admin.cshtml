@{
}
@model Diemdanh
<html lang="en">
<body>
   <table style="width:100%">
     <tr>
         <th>ID</th>
         <th>ID_NV</th>
         <th>NAME</th>
         <th>TIME_IN</th>
         <th>TIME_OUT</th>
     </tr>
     @{
         List<Diemdanh> diemdanhlist = ViewData["diemdanhList"] as List<Diemdanh>;
             @foreach (Diemdanh staff in diemdanhlist)
             {
                <tr>
                    <td> @staff.id</td>
                    <td> @staff.id_nv</td>
                    <td> @staff.name</td>
                    <td> @staff.timein</td>
                    <td> @staff.timeout</td>
                    <td>
                       <form role="form" method="post">
                         <button asp-controller="" asp-action="" id="@staff.id" class="duỵet">sửa</button>
                        </form>
                     </td>
                </tr>
             }
      }
     <tbody id="tbody"></tbody>
   </table>
    <script src="~/js/dim.js"></script>
</body>
</html>