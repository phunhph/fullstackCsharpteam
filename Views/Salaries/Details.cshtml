@model fullstackCsharp.Models.Salary

@{
    ViewData["Title"] = "Details";
}
<h3>Chi tiết</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên nhân viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdUNavigation.FullName)
        </dd>
        <dt class="col-sm-2">
            Lương cơ bản
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BasicSalary)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.IdSalary">Sửa</a> |
    <a class="btn btn-outline-info" asp-action="Index">Trở lại</a>
</div>
