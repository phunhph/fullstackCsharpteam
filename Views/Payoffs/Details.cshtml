@model fullstackCsharp.Models.Payoff

@{
    ViewData["Title"] = "Payoff Details ";
}

<h1>Details</h1>

<div>
    <h4>Payoff</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên nhân viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdUNavigation.IdU)
        </dd>
        <dt class = "col-sm-2">
            Số tiền Thưởng - Phạt
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Payoff1)
        </dd>
        <dt class = "col-sm-2">
            Ngày tạo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PayoffDate)
        </dd>
        <dt class = "col-sm-2">
            Mô tả
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.IdPay">Sửa</a> |
    <a class="btn btn-info" asp-action="Index">Trở lại</a>
</div>
